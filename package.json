{
	"name": "nuxt-better-auth",
	"type": "module",
	"version": "0.3.3",
	"packageManager": "pnpm@10.18.3",
	"author": "zhong666 <hi@zhong666.me>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/aa900031/nuxt-better-auth"
	},
	"keywords": [
		"nuxt",
		"better-auth",
		"auth",
		"nuxt-module"
	],
	"exports": {
		".": {
			"types": "./dist/types.d.mts",
			"import": "./dist/module.mjs"
		}
	},
	"main": "./dist/module.mjs",
	"typesVersions": {
		"*": {
			".": [
				"./dist/types.d.mts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"postinstall": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
		"build": "nuxt-module-build build",
		"dev": "nuxi dev playground",
		"dev:build": "nuxi build playground",
		"release": "release-it",
		"lint": "eslint .",
		"test": "vitest run",
		"test:ci": "vitest run --coverage.provider=istanbul --reporter=junit --outputFile.junit=./reports/junit.xml",
		"test:watch": "vitest watch",
		"test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
	},
	"peerDependencies": {
		"better-auth": "^1.2.0"
	},
	"dependencies": {
		"@nuxt/kit": "catalog:deps",
		"defu": "catalog:deps",
		"knitwork": "catalog:deps",
		"nitro-better-auth": "catalog:deps",
		"pathe": "catalog:deps"
	},
	"devDependencies": {
		"@aa900031/eslint-config": "catalog:dev",
		"@aa900031/release-it-config": "catalog:dev",
		"@nuxt/devtools": "catalog:dev",
		"@nuxt/eslint-config": "catalog:dev",
		"@nuxt/module-builder": "catalog:dev",
		"@nuxt/schema": "catalog:dev",
		"@nuxt/test-utils": "catalog:dev",
		"@types/node": "catalog:types",
		"@vitest/coverage-istanbul": "catalog:dev",
		"better-auth": "catalog:peer",
		"eslint": "catalog:dev",
		"release-it": "catalog:dev",
		"typescript": "catalog:types",
		"vitest": "catalog:dev",
		"vue-tsc": "catalog:types"
	}
}
