{
	"name": "nuxt-better-auth",
	"type": "module",
	"version": "0.4.0",
	"packageManager": "pnpm@10.18.3",
	"author": "zhong666 <hi@zhong666.me>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/aa900031/nuxt-better-auth"
	},
	"keywords": [
		"nuxt",
		"better-auth",
		"auth",
		"nuxt-module"
	],
	"exports": {
		".": {
			"types": "./dist/types.d.mts",
			"import": "./dist/module.mjs"
		}
	},
	"main": "./dist/module.mjs",
	"typesVersions": {
		"*": {
			".": [
				"./dist/types.d.mts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "nuxt-module-build prepare && nuxt-module-build build",
		"dev": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi dev playground",
		"dev:build": "nuxi build playground",
		"release": "release-it",
		"lint": "eslint .",
		"test": "vitest run",
		"test:ci": "nuxt-module-build build --stub && nuxt-module-build prepare && vitest run --coverage.provider=istanbul --reporter=junit --outputFile.junit=./reports/junit.xml",
		"test:watch": "vitest watch",
		"test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
	},
	"peerDependencies": {
		"better-auth": "^1.2.0"
	},
	"dependencies": {
		"@nuxt/kit": "^3.19.3",
		"defu": "^6.1.4",
		"knitwork": "^1.2.0",
		"nitro-better-auth": "^0.2.1",
		"pathe": "^2.0.3"
	},
	"devDependencies": {
		"@aa900031/eslint-config": "^3.2.0",
		"@aa900031/release-it-config": "^0.1.0",
		"@nuxt/devtools": "^2.6.5",
		"@nuxt/eslint-config": "^1.9.0",
		"@nuxt/module-builder": "^1.0.2",
		"@nuxt/schema": "^3.19.3",
		"@nuxt/test-utils": "^3.19.2",
		"@types/node": "^20.19.23",
		"@vitest/coverage-istanbul": "^3.2.4",
		"better-auth": "^1.3.28",
		"eslint": "^9.38.0",
		"release-it": "^19.0.5",
		"typescript": "~5.9.3",
		"vitest": "^3.2.4",
		"vue-tsc": "^2.2.12"
	}
}
